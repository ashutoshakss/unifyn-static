{
  "name": "unifyn-trade",
  "version": "1.1.0",
  "description": "Global unified, broker-agnostic trading hub",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "serve:dist": "npx http-server ./out -p 8080 -c-1",
    "preview": "wrangler pages dev out",
    "deploy": "wrangler pages deploy out --project-name=unifyn-trade",
    "deploy:s3": "npm run build && aws s3 sync out s3://YOUR-BUCKET-NAME --delete --cache-control max-age=31536000,public --exclude '*.html' && aws s3 sync out s3://YOUR-BUCKET-NAME --delete --cache-control max-age=3600,public --exclude '*' --include '*.html'",
    "validate": "node scripts/validate.js"
  },
  "keywords": [
    "unified finance superapp",
    "broker agnostic trading app",
    "unified finance",
    "broker agnostic platform",
    "multi broker trading",
    "global trading platform",
    "multi-broker integration (planned)",
    "unified trading interface",
    "fintech superapp",
    "broker independent trading",
    "consolidated trading platform",
    "portfolio management",
    "trading analytics platform",
    "multi-broker app"
  ],
  "author": "Unifyn",
  "license": "UNLICENSED",
  "dependencies": {
    "next": "^15.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "24.9.1",
    "@types/react": "19.2.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "typescript": "5.9.3",
    "wrangler": "^3.22.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ]
}
